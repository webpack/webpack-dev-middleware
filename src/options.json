{
  "type": "object",
  "properties": {
    "logLevel": {
      "enum": ["info", "warn", "error", "debug", "trace", "silent"]
    },
    "logTime": {
      "type": "boolean"
    },
    "logger": {
      "type": "object"
    },
    "mimeTypes": {
      "type": "object"
    },
    "stats": {
      "anyOf": [
        {
          "type": "object"
        },
        {
          "type": "boolean"
        },
        {
          "enum": [
            "none",
            "errors-only",
            "errors-warnings",
            "minimal",
            "normal",
            "verbose"
          ]
        }
      ]
    },
    "watchOptions": {
      "type": "object"
    },
    "writeToDisk": {
      "anyOf": [
        {
          "type": "boolean"
        },
        {
          "instaceof": "Function"
        }
      ]
    },
    "methods": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "headers": {
      "type": "object"
    },
    "lazy": {
      "type": "boolean"
    },
    "filename": {
      "type": "string"
    },
    "publicPath": {
      "type": "string"
    },
    "serverSideRender": {
      "type": "boolean"
    },
    "outputFileSystem": {
      "type": "object"
    },
    "index": {
      "anyOf": [
        {
          "type": "boolean"
        },
        {
          "type": "string"
        }
      ]
    }
  },
  "additionalProperties": false
