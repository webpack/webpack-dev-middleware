{
    "env": {
        "node": true
    },
    "rules": {
        "strict": 0,
        "camelcase": 0,
        "curly": 0,
        "indent": [2, "tab"],
        "eol-last": 2,
        "no-shadow": 0,
        "no-redeclare": 2,
        "no-extra-bind": 2,
        "no-empty": 0,
        "no-process-exit": 2,
        "no-underscore-dangle": 0,
        "no-use-before-define": 0,
        "no-unused-vars": 2,
        "consistent-return": 0,
        "no-inner-declarations": 2,
        "no-loop-func": 2,
        "no-undef": 2,
        "space-before-function-paren": [2, "never"]
    }
}
